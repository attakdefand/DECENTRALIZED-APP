{
  "dashboard": {
    "id": null,
    "title": "DApp SLO Dashboard",
    "tags": ["slo", "monitoring"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "type": "graph",
        "title": "API Latency (95th Percentile)",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "API Latency (p95)",
            "refId": "A"
          },
          {
            "expr": "0.25",
            "legendFormat": "SLO Target (250ms)",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Latency (seconds)"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 2,
        "type": "graph",
        "title": "API Error Rate",
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "(sum(rate(http_request_errors_total[5m])) / sum(rate(http_requests_total[5m]))) * 100",
            "legendFormat": "Error Rate (%)",
            "refId": "A"
          },
          {
            "expr": "0.5",
            "legendFormat": "SLO Target (0.5%)",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "percent",
            "label": "Error Rate (%)"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 3,
        "type": "graph",
        "title": "Indexer Lag",
        "gridPos": {
          "x": 0,
          "y": 8,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "indexer_lag_blocks",
            "legendFormat": "Indexer Lag (blocks)",
            "refId": "A"
          },
          {
            "expr": "2",
            "legendFormat": "SLO Target (2 blocks)",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Blocks"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 4,
        "type": "graph",
        "title": "IPFS Pin Coverage",
        "gridPos": {
          "x": 12,
          "y": 8,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "ipfs_pin_coverage_percent",
            "legendFormat": "Pin Coverage (%)",
            "refId": "A"
          },
          {
            "expr": "99",
            "legendFormat": "SLO Target (99%)",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "percent",
            "label": "Coverage (%)"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 5,
        "type": "graph",
        "title": "MEV Incidents",
        "gridPos": {
          "x": 0,
          "y": 16,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "increase(mev_incidents_total[1h])",
            "legendFormat": "MEV Incidents (per hour)",
            "refId": "A"
          },
          {
            "expr": "0",
            "legendFormat": "SLO Target (0)",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Incidents"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 6,
        "type": "stat",
        "title": "Error Budget Remaining - API Latency",
        "gridPos": {
          "x": 12,
          "y": 16,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "100 - (histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[7d])) by (le)) / 0.25) * 100",
            "refId": "A"
          }
        ],
        "options": {
          "fieldOptions": {
            "defaults": {
              "unit": "percent"
            }
          }
        }
      },
      {
        "id": 7,
        "type": "stat",
        "title": "Error Budget Remaining - API Error Rate",
        "gridPos": {
          "x": 18,
          "y": 16,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "100 - ((sum(rate(http_request_errors_total[7d])) / sum(rate(http_requests_total[7d]))) * 100 / 0.5) * 100",
            "refId": "A"
          }
        ],
        "options": {
          "fieldOptions": {
            "defaults": {
              "unit": "percent"
            }
          }
        }
      }
    ]
  }
}